#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char linha[1000];

    while (1) {
        fgets(linha, sizeof(linha), stdin);

        // Remover \n do final
        linha[strcspn(linha, "\n")] = '\0';

        // Condição de parada
        if (linha[0] == '*')
            break;

        // Quebrar a frase em palavras
        char *palavra = strtok(linha, " ");
        char inicial = tolower(palavra[0]); // primeira letra da primeira palavra
        int tautograma = 1;

        while (palavra != NULL) {
            if (tolower(palavra[0]) != inicial) {
                tautograma = 0;
                break;
            }
            palavra = strtok(NULL, " ");
        }

        if (tautograma)
            printf("Y\n");
        else
            printf("N\n");
    }

    return 0;
}